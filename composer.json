{
    "name": "maxalmonte14/nfl-scores",
    "description": "A CLI for getting NFL games real-time data.",
    "keywords": ["terminal", "nfl", "sports", "cli"],
    "license": "MIT",
    "type": "project",
    "support": {
        "issues": "https://github.com/maxalmonte14/nfl-scores/issues",
        "source": "https://github.com/maxalmonte14/nfl-scores"
    },
    "authors": [
        {
            "name": "Max Almonte",
            "email": "maxalmonte14@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "guzzlehttp/guzzle": "^6.3",
        "laravel-zero/framework": "5.6.*",
        "maxalmonte14/phpcollections": "^0.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "files": ["helpers.php"],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "ext-posix": "0"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["nfl-scores"]
}
